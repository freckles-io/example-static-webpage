- freckle:
    owner: www-data                    # optional, if not provided, uses the current owner of the freckle folder
    group: www-data                    # optional

- static-website:
    static_website_domain: 127.0.0.1   # ip address or domain name used by this server
    static_website_port: 80            # port the webserver listens to
    letsencrypt_email: none            # if not 'none', the adapter will request a letsencrypt https certificate, and use this email address in the process
